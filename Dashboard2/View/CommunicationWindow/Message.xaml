<Window x:Class="Dashboard2.View.CommunicationWindow.Message"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dashboard2.View.CommunicationWindow"
        mc:Ignorable="d"
        Title="Message" Height="150" Width="255">
    <Grid Margin="0,0,0,1">
        <TextBox x:Name="TextBoxMessage" HorizontalAlignment="Center" Margin="0,15,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="204" Height="42"/>
        <Button Content="OK" HorizontalAlignment="Center" Margin="0,90,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>

</Window>
